<physical_partition type="spinand">
    <!--Uboot will ues 9th block to store a back up of fip.bin.
    Please keep the first 2.5MB reserved for fip and fip size less than 1.25MB-->
    <partition label="fip" size_in_kb="2560" file="fip.bin"/>
    <partition label="ENV" size_in_kb="256" file="" />
    <partition label="ENV_BAK" size_in_kb="256" file="" />
    <partition label="BOOT" size_in_kb="4096" file="boot.spinand"/>
    <partition label="ROOTFS" size_in_kb="92160" file="rootfs.spinand" />
    <partition label="SYSTEM" size_in_kb="10240" file="system.spinand" mountpoint="" type="ubifs" />
    <partition label="DATA" file="data.spinand" mountpoint="/mnt/data" type="ubifs" />
</physical_partition>
